#include<iostream>
#include<math.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    int o=0,e=0;
    for(int i=0;i<n;i++)
    {
         long e;
        cin>>e;
        if(e&1==0)
        e++;
        else
        o++;

    }

    if(n&1==0)
    {
        if(e==o)
        cout<<"DL"<<endl;
        else cout<<"DETAIN"<<endl;
    }
    else {
        if(abs(e-o)==1 or e==o){
            std::cout << "DL" << '\n';
        }
        else cout<<"DETAIN"<<endl;


    }
return 0;}
